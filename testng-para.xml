<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" >
<listeners>
		<listener class-name="com.Utility.framework.Reporting" />
	</listeners>
  <test name="Test-firefox">
		<parameter name="browser" value="firefox"></parameter>
		<classes >
			<class name="com.TestCases.framework.SearchTextFireFox" />
		</classes>
	</test>
	<test name="Test-chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.TestCases.framework.SearchTextChrome" />
		</classes>
	</test>
</suite> 